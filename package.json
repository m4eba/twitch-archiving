{
  "name": "twitch-archiving",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "config/*",
    "libraries/*",
    "tools/*",
    "microservices",
    "api"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^2.8.4",
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.3.1"
}
